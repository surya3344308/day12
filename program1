#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>
void print_pattern(int n) {
    for (int i = 1; i <= n; i++) {
        int start = (i % 2 == 0) ? 2 : 1; 
        for (int j = 1; j <= i; j++) {
            printf("%d", start);
            start += 2; // 
        }
        printf("\n");
    }
}

int main() {
    int n;
    scanf("%d", &n); 
    print_pattern(n);
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}
